
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Add Material</title>
    <!-- Bootstrap Core CSS -->

    <link href="@Url.Content("~/Content/CSS/bootstrap.min.css")" rel="stylesheet" />
    <!-- Custom CSS -->

    <link href="@Url.Content("~/Content/CSS/sb-admin.css" )" rel="stylesheet" />
    <!-- Morris Charts CSS -->

    <link href="@Url.Content("~/Content/CSS/morris.css")" rel="stylesheet" />
    <!-- Custom Fonts -->
    <link href="@Url.Content("~/Content/font-awesome.min.css")" rel="stylesheet" />
   
    <link href="@Url.Content("~/Content/CSS/bootstrap-select.min.css")" rel="stylesheet" />


    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <link rel="stylesheet" href="f"/>
    <![endif]-->
    <script src="@Url.Content("~/Ajax Scripts/Paper.js")"></script>
    <script src="@Url.Content("~/Ajax Scripts/Material.js")"></script>
</head>
<body>
    <div id="wrapper">

        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

                <a class="navbar-brand" href="@Url.Action("Index","TeacherDashboard")">Teacher Panel</a>
            </div>
            <!-- Top Menu Items -->
            <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li class="active">
                        <a href="@Url.Action("Add","Material")"><i class="fa fa-fw fa-table"></i>Material</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Index","TeacherDashboard")"><i class="fa fa-fw fa-dashboard"></i>Back to Dashboard</a>
                    </li>


                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </nav>

        <div id="page-wrapper">

            <div class="container-fluid">

                <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">
                            Add Material
                        </h1>
                        <ol class="breadcrumb">
                            <li class="active">
                                <i class="fa fa-dashboard"></i> Dashboard
                            </li>
                        </ol>
                    </div>
                </div>

                <!-- /.row -->

                <div class="row">
                    <div class="col-md-4">

                        <div class="form-group">


                            <h4>
                                <label for="session">

                                    <span class="label label-primary">Session ID</span>

                                </label>
                            </h4>
                            <select class="selectpicker" onchange="getSemestersById(this.value)" id="sessionSelectOptions"></select>
                            <h4>
                                <label for="semesterSelectOption">

                                    <span class="label label-primary">Semester ID</span>

                                </label>
                            </h4>
                            <select class="selectpicker" id="semesterSelectOption" onchange="getCourse()"></select>
                        </div>
                        <div class="form-group">
                            <h4>
                                <label for="subjectSelectOptions">

                                    <span class="label label-primary">Course ID</span>

                                </label>
                            </h4>
                            <select class="selectpicker" id="subjectSelectOptions" onchange="getTeachers()"></select>

                        </div>

                        <div class="form-group">
                            <h4>
                                <label for="teacherSelectOptions">

                                    <span class="label label-primary">Teacher</span>

                                </label>
                            </h4>


                            <select class="selectpicker" id="teacherSelectOptions"></select>
                        </div>
                        <div class="form-group">
                            <h4>

                                <label><span class="label label-primary">Material Name</span></label>

                            </h4>
                            <input type="text" class="form-control" id="materialName" maxlength="100" />
                        </div>
                        <div class="form-group">
                            <h4>
                                <label><span class="label label-primary">Material Description</span></label>

                            </h4>
                            <input type="text" class="form-control" id="materialDescription" maxlength="100" />
                        </div>

                        <br />
                        <input type="button" class="btn btn-default" id="submitBtn" value="Save" onClick="saveMaterial()" />


                    </div>
                </div>
            </div>
        </div>
    </div>


    <script src="@Url.Content("~/Script/jquery.js")"></script>
    <!-- Bootstrap Core JavaScript -->

    <script src="@Url.Content("~/Script/bootstrap.min.js")"></script>

    <!-- Morris Charts JavaScript -->

    <script src="@Url.Content("~/Script/raphael.min.js")"></script>

    <script src="@Url.Content("~/Script/morris.min.js")"></script>

    <script src="@Url.Content("~/Script/morris-data.js")"></script>
    <script src="@Url.Content("~/Script/bootstrap-select.min.js")"></script>
    <script>getSessions()</script>

</body>
</html>
