                </button>
                <a class="navbar-brand" href="@Url.Action("Index","TeacherDashboard")">Teacher Panel</a>
            </div>
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li class="active">
                        <a href="@Url.Action("Folder","Student")"><i class="fa fa-fw fa-dashboard"></i>Material</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Dashboard","Student")"><i class="fa fa-fw fa-dashboard"></i>Back Dashboard</a>
                    </li>
