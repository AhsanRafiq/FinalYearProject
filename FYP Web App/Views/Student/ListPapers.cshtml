<!DOCTYPE html>

<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>List Of Papers</title>

    <!-- Bootstrap Core CSS -->

    <link href="@Url.Content("~/Content/CSS/bootstrap.min.css")" rel="stylesheet" />
    <!-- Custom CSS -->

    <link href="@Url.Content("~/Content/CSS/sb-admin.css")" rel="stylesheet" />
    <!-- Morris Charts CSS -->

    <link href="@Url.Content("~/Content/CSS/morris.css")" rel="stylesheet" />
    <!-- Custom Fonts -->
    <link href="@Url.Content("~/Content/CSS/bootstrap-select.min.css")" rel="stylesheet" />


    <link href="@Url.Content("~/Content/font-awesome.min.css")" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <link rel="stylesheet" href="f"/>
    <![endif]-->
    <script src="@Url.Content("~/Ajax Scripts/Student.js")"></script>
</head>

<body>
    <div id="wrapper">
        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("Dashboard","Student")">Student Panel</a>
            </div>
            <!-- Top Menu Items -->

            <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li class="active">
                        <a href="@Url.Action("ListPaper","Student")"><i class="fa fa-fw fa-dashboard"></i>Paper</a>
                    </li>
                    <li>
                        <a href="@Url.Action("Dashboard","Student")"><i class="fa fa-fw fa-dashboard"></i>Back Dashboard</a>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </nav>
        <div id="page-wrapper" style="height: 100%">
            <div class="container-fluid">
               <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">
                            View Paper
                        </h1>
                        <ol class="breadcrumb">
                            <li class="active">
                                <i class="fa fa-dashboard"></i> Please Select the option below
                            </li>
                        </ol>
                    </div>
                </div>
                <!-- /.row -->
                <h4 style="display: inline-block">
                    <label for="sessionSelectOptions">
                       <span class="label label-primary">Session ID</span>
                    </label>
                </h4>
                <select style="display: inline-block" class="selectpicker" onchange="getSemestersById(this.value)" id="sessionSelectOptions"></select>
                <h4 style="display: inline-block">
                    <label for="semesterSelectOption">
                        <span class="label label-primary">Semester ID</span>
                    </label>
                </h4>
                <select style="display: inline-block" class="selectpicker" id="semesterSelectOption"></select>
                <input id="teacherId" value="4" style="visibility: hidden" />
                <button onclick="displayPaperForTest()">Get Info</button>
            </div>
            <br />
            <br />
            <div class="row">
                <div>
                    <table class="table table-striped table-bordered table-hover table-condensed table-responsives text-center">

                        <thead class="thead-inverse">
                            <tr>
                                <th class="text-center">Course Name</th>
                                <th class="text-center">Paper Name</th>
                                <th class="text-center">No.of Questions</th>
                                <th class="text-center">Time Allowed</th>
                                <th class="text-center">Total Marks</th>
                                <th class="text-center">Actions</th>
                            </tr>
                        </thead>

                        <tbody id="tbody"></tbody>

                    </table>



                </div>
            </div>
        </div>
    </div>




    <script src="@Url.Content("~/Ajax Scripts/Paper.js")"></script>

    <script src="@Url.Content("~/Script/jquery.js")"></script>
    <!-- Bootstrap Core JavaScript -->
    <!-- Morris Charts JavaScript -->
    <script src="@Url.Content("~/Script/bootstrap.min.js")"></script>

    <script src="@Url.Content("~/Script/raphael.min.js")"></script>

    <script src="@Url.Content("~/Script/morris.min.js")"></script>

    <script src="@Url.Content("~/Script/morris-data.js")"></script>

    <script src="@Url.Content("~/Script/bootstrap-select.min.js")"></script>

    <script>getSessions();</script>



    <div class="modal fade" id="promptPassword" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Modal Header</h4>
                </div>
                <div class="modal-body">
                    <label>
                        <span class="label label-primary">Enter Password</span>
                    </label>
                    <input type="text" id="testPassword" class="form-control" maxlength="10" />

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-default" onclick="startTest()" data-dismiss="modal">Match Password</button>
                </div>
            </div>

        </div>
    </div>

    <div class="modal fade" id="startTest" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Modal Header</h4>
                </div>
                <div class="modal-body">
                    <label>
                        <span class="label label-primary">Password Matched</span>
                    </label>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-default" onclick="location.href='@Url.Action("Test", "Student")'" data-dismiss="modal">Start Test</button>
                </div>
            </div>

        </div>
    </div>






</body>

</html>
